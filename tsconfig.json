{
    "compilerOptions": {
        "target": "es6",
        "strict": true,
        "module": "commonjs",
        "sourceMap": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "forceConsistentCasingInFileNames": true, // Enable consistent casing in filenames
        "paths": {
            "@pages/*": [
                "pom/letcodemodules/credentials/*"
            ],
            "@fixtures/*": [
                "fixtures/*"
            ],
            "@files/*": [
                "uploadFolder/*"
            ]
        }
    }
}
